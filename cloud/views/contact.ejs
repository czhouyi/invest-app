
<div class="row">
  <div class="col-md-12">
    <h3>好友列表</h3>
    <table class="table table-borderless table-sortable">
      <thead>
      <tr>
        <th data-sort="string">姓名</th>
        <th data-sort="string" class="text-center">发起项目</th>
        <th data-sort="string" class="text-center">参与项目</th>
        <th data-sort="string" class="text-center">完成项目</th>
        <th data-sort="string" class="text-center">邀请我参与项目</th>
        <th></th>
      </tr>
      </thead>
      <tbody>
      <% users.forEach(function (user){ %>
      <tr>
        <td>
          <%= user.realName %>
        </td>
        <td class="text-center text-bold"><%= user.ccnt %></td>
        <td class="text-center text-bold"><%= user.gcnt %></td>
        <td class="text-center text-bold"><%= user.pcnt %></td>
        <td class="text-center text-bold"><%= user.mcnt %></td>
        <td><a href="/contact/profile/<%= user.id %>" class="btn btn-primary btn-xs">个人主页</a></td>
      </tr>
      <% }); %>
      </tbody>
    </table>
  </div>
</div>
